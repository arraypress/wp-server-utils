{
  "name": "arraypress/wp-server-utils",
  "version": "1.0.0",
  "description": "A lean WordPress library for server environment detection and basic system information",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-server-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\ServerUtils\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-server-utils/issues",
    "source": "https://github.com/arraypress/wp-server-utils"
  },
  "keywords": [
    "wordpress",
    "server",
    "environment",
    "hosting",
    "php",
    "system",
    "detection",
    "arraypress",
    "server-info",
    "environment-detection"
  ]
}